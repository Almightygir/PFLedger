{{log 'ledgers' this}}
<form>
  <ul>
    <table>
      <thead>
        <tr>
          <th>Log</th>
          <th>Currency:</th>
          <th>CP</th>
          <th>SP</th>
          <th>GP</th>
          <th>PP</th>
          <th>Weightless:</th>
          <th>CP</th>
          <th>SP</th>
          <th>GP</th>
          <th>PP</th>
          <th>Character</th>
          <th>User</th>
        </tr>
      </thead>
      <tbody>
        {{#each ledgers}}
          <tr>
            <td>{{Log}}</td>
            <td> </td>
            <td>{{CP}}</td>
            <td>{{SP}}</td>
            <td>{{GP}}</td>
            <td>{{PP}}</td>
            <td> </td>
            <td>{{altCP}}</td>
            <td>{{altSP}}</td>
            <td>{{altGP}}</td>
            <td>{{altPP}}</td>
            <td>{{Character}}</td>
            <td>{{UserName}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </ul>
</form>
